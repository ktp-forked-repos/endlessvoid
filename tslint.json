{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [true, "allow-leading-underscore", "allow-snake-case"],
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "semicolon": [true, "never"],
        "arrow-parens": [true, "ban-single-arg-parens"]
    },
    "rulesDirectory": []
}